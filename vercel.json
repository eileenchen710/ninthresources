{
  "version": 2,
  "name": "komito-co-working-template",
  "buildCommand": "echo 'No build required for static HTML'",
  "outputDirectory": "Komito Pack/Komito",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 